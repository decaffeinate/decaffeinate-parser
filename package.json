{
  "name": "decaffeinate-parser",
  "version": "0.0.0-development",
  "description": "A better AST for CoffeeScript, inspired by CoffeeScriptRedux.",
  "keywords": [
    "ast",
    "coffeescript",
    "parse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/decaffeinate/decaffeinate-parser.git"
  },
  "license": "MIT",
  "author": "Brian Donovan",
  "main": "dist/parser.js",
  "module": "dist/parser.mjs",
  "types": "dist/parser.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "./script/build",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "npm run lint --fix",
    "pretest": "npm run lint",
    "test": "jest",
    "test:ci": "jest --ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md,json}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/types": "^7.18.4",
    "@codemod/parser": "^1.2.1",
    "coffee-lex": "^9.1.5",
    "decaffeinate-coffeescript": "^1.12.7-patch.3",
    "decaffeinate-coffeescript2": "^2.2.1-patch.5",
    "lines-and-columns": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^6.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.57.0",
    "string-repeat": "^1.1.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
