{
  "name": "decaffeinate-parser",
  "version": "0.0.0-development",
  "description": "A better AST for CoffeeScript, inspired by CoffeeScriptRedux.",
  "keywords": [
    "ast",
    "coffeescript",
    "parse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/decaffeinate/decaffeinate-parser.git"
  },
  "license": "MIT",
  "author": "Brian Donovan",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "pnpm lint --fix",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "pretest": "pnpm lint",
    "test": "jest",
    "test:ci": "jest --ci"
  },
  "lint-staged": {
    "*.{ts,md,json}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@babel/types": "^7.18.4",
    "@codemod/parser": "^1.2.1",
    "coffee-lex": "^9.3.1",
    "decaffeinate-coffeescript": "^1.12.7-patch.4",
    "decaffeinate-coffeescript2": "^2.2.1-patch.6",
    "lines-and-columns": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.57.0",
    "string-repeat": "^1.1.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
